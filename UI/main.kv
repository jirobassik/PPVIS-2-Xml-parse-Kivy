<MainWindow>:
    name: "main"
    main_label: main
    second_label: hind_label

    FloatLayout:
        ScrollView:
            #size_hint_y: 1
            #size_hint_x: 0.9
            size_hint: (None, None)
            size: (1050, 500)
            pos_hint: {"x": 0.17, 'y': 0.3}
            Label:
                id: main
                text: root.main_labels
                halign: 'left'
                valign: 'top'
                size_hint: None, None
                size: self.texture_size
                font_name: 'DroidSansMono'
                background_color: (0, 0, 1, 0.1)
                canvas.before:
                    Color:
                        rgba: self.background_color
                    Rectangle:
                        size: self.size
                        pos: self.pos

        Label:
            text: "Название товара"
            size_hint: 0.155, 0.04
            pos_hint: {"x": 0.17, 'y': 0.88}
            background_color: (0, 0, 1, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Label:
            text: "Название производителя"
            size_hint: 0.155, 0.04
            pos_hint: {"x": 0.326, 'y': 0.88}
            background_color: (0, 0, 1, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Label:
            text: "УНП товара"
            size_hint: 0.088, 0.04
            pos_hint: {"x": 0.483, 'y': 0.88}
            background_color: (0, 0, 1, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Label:
            text: "Количество товара"
            size_hint: 0.125, 0.04
            pos_hint: {"x": 0.573, 'y': 0.88}
            background_color: (0, 0, 1, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Label:
            text: "Адрес склада"
            size_hint: 0.137, 0.04
            pos_hint: {"x": 0.7, 'y': 0.88}
            background_color: (0, 0, 1, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Label:
            id: hind_label
            text: root.second_labels
            size_hint: 0.3, 0.07
            pos_hint: {"x": 0.35, 'y': 0.22}
            background_color: (0, 0, 0, 0.1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos


    BoxLayout:

        Button:
            text: "Удаление"
            background_color: 0.1, 0.5, 0.6, 1
            size_hint: 0.55, 0.2
            on_release:
                app.root.current = "Delete"
                root.manager.transition.direction = "right"

        Button:
            text: "Добавление"
            background_color: 0.1, 0.5, 0.6, 1
            size_hint: 0.55, 0.2
            on_release:
                app.root.current = "Add"
                root.manager.transition.direction = "up"

        Button:
            text: "Обновление"
            background_color: 0.1, 0.5, 0.6, 1
            size_hint: 0.55, 0.2
            on_press: root.update_data()

        Button:
            text: "Поиск"
            background_color: 0.1, 0.5, 0.6, 1
            size_hint: 0.55, 0.2
            on_release:
                app.root.current = "Search"
                root.manager.transition.direction = "left"

